{
  "manifest_version": 2,
  "name": "Thingiverse Analytics Downloader",
  "version": "0.2",
  "description": "Get your analytics data from Thingiverse!",
  "content_scripts": [
    {
      "matches": [
        "https://www.thingiverse.com/*/analytics",
        "https://www.thingiverse.com/*/analytics/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "thingi.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}